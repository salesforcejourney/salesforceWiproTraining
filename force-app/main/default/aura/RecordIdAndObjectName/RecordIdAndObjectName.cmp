<aura:component
  implements="flexipage:availableForAllPageTypes,force:hasSObjectName,force:hasRecordId"
>
  <aura:handler
    name="init"
    value="{!this}"
    action="{!c.initHandler}"
  ></aura:handler>
  <lightning:card title="Record Id and Object Name" iconName="custom:custom14">
    <p>Object Name : {!v.sObjectName}</p>
    <p>Record Id : {!v.recordId}</p>
  </lightning:card>
</aura:component>
